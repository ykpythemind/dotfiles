{
  /*"coc.preferences.useQuickfixForLocations": true,*/
  "coc.preferences.jumpCommand": "edit",
 /* "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "Markdown",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "typescript.tsx",
    "json",
    "go"
  ],*/
  "suggest.floatEnable": true,
  "suggest.maxCompleteItemCount": 30,
  "suggest.noselect": true,
  "suggest.keepCompleteopt": true,
  "languageserver": {
    "go": {
      "enable": true,
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/"],
      "trace.server": "verbose",
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
  "snippets.ultisnips.enable": false,
  "snippets.loadFromExtensions": false,
  "snippets.priority": 100,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "typescriptreact": ["javascript"]
  }
}
